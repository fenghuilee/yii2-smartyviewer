{
    "name": "fenghuilee/yii2-smartyviewer",
    "description": "The Smarty Viewer integration for the Yii framework 2+",
    "keywords": ["yii2", "smarty", "renderer"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fenghuilee/yii2-smartyviewer/issues",
        "source": "https://github.com/fenghuilee/yii2-smartyviewer"
    },
    "authors": [
        {
            "name": "Fenghui Lee",
            "email": "dev@fenghuilee.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "smarty/smarty": "*"
    },
    "autoload": {
        "psr-4": { "yii\\smartyviewer\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
